	"blocks": "never",
	"brackets": "never",
	"colons": "never",
	"colors": "always",
	"commaSpace": "always",
	"commentSpace": "always",
	"cssLiteral": "never",
	"depthLimit": 5,
	"duplicates": true,
	"efficient": "always",
	"extendPref": "@extend",
	"globalDupe": true,
	"groupOutputByFile": true,
	"indentPref": false,
	"leadingZero": "never",
	"maxErrors": false,
